{
    "parser": "@typescript-eslint/parser",
    "plugins": ["import", "@typescript-eslint"],
    "extends": [
        "react-app",
        "react-app/jest",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "no-debugger": 1,
        "no-console": 1,
        "semi": 1,
        "@typescript-eslint/no-explicit-any": 2
    }
}